<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.firstzone.owner">
	<select id="selectByOwnerId" resultType="ownervo" parameterType="String">
		select * 
		from OWNER
		where owner_id = #{aa}
	</select>
</mapper>